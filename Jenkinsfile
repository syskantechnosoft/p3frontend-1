      steps {
        bat 'Build bat'
        echo "Compiled Success!"
      }
    }
  
      stage ('JUnit') {
      steps {
        echo "Testing Passed!"
      }
    }
  
      stage ('Quality-Gate') {
      steps {
        echo "Quality Gate passed!"
      }
    }
  
  
  
        }


        }
